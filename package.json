{
  "name": "openmrs-module-ipd-frontend",
  "version": "0.0.8",
  "description": "React frontend for IPD module",
  "main": "dist/index.js",
  "author": "bahmni@thoughtworks.com",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.5",
    "@types/carbon-components-react": "^7.55.1",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "5.2.6",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.6",
    "moment": "^2.29.4",
    "prettier": "^2.8.8",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.63.4",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@storybook/addon-styling": "0.3",
    "@storybook/react": "6.5",
    "bahmni-carbon-ui": "0.0.9",
    "carbon-components": "^10.19.0",
    "carbon-components-react": "^7.25.0",
    "carbon-icons": "^7.0.7",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-intl": "^3.3.2"
  },
  "scripts": {
    "preinstall": "git config core.hooksPath .git-hooks",
    "start-storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:ci": "yarn test --ci",
    "bump:patch": "yarn version --no-git-tag-version --patch",
    "bump:minor": "yarn version --no-git-tag-version --minor",
    "bump:major": "yarn version --no-git-tag-version --major",
    "lint": "eslint src/**/*.{js,jsx}",
    "dev:sandbox": "webpack serve --mode development --open --config-name=sandbox",
    "dev:integrated": "concurrently \"yarn build --watch\" \"yarn serve:prod\"",
    "build": "webpack --mode production --config-name=federation",
    "serve:prod": "cd dist/federation && http-server -p=9001 -P=http://localhost:8080 -c-1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./styles.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.!(js|jsx)": [
      "prettier --write --ignore-unknown"
    ]
  }
}
